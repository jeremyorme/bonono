{
    "name": "bonono-react",
    "sideEffects": false,
    "version": "0.4.0",
    "description": "React specific wrapper for Bonono",
    "repository": {
        "type": "git",
        "url": "https://github.com/jeremyorme/bonono"
    },
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rmdir /Q /S dist 2>nul & cmd /c \"exit /b 0\"",
        "compile": "npm run tsc",
        "tsc": "tsc -p .",
        "rollup": "rollup -c"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "devDependencies": {
        "@types/jest": "23.3.9",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "jest": "^23.0.0",
        "jest-dom": "^3.0.2",
        "np": "^3.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "bonono": "^0.4.0"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
        "testPathIgnorePatterns": [
            "node_modules",
            "dist"
        ]
    }
}
